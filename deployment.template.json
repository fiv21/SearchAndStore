{
  "$schema-template": "1.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "iaepractiacontainerregistry": {
                "username": "$CONTAINER_REGISTRY_USERNAME_iaepractiacontainerregistry",
                "password": "$CONTAINER_REGISTRY_PASSWORD_iaepractiacontainerregistry",
                "address": "iaepractiacontainerregistry.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "FaceDetector": {
            "version": "12.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "env": {
              "rtspUser1": {
                "value": "flanders"
              },
              "rtspPass1": {
                "value": "flanders123"
              },
              "IPCam1": {
                "value": "192.168.1.125"
              },
              "PortCam1": {
                "value": "443"
              },
              "PathCam1": {
                "value": "/videoMain"
              },
              "rtspUser2": {
                "value": "admin"
              },
              "rtspPass2": {
                "value": "admin"
              },
              "IPCam2": {
                "value": "192.168.1.115"
              },
              "PortCam2": {
                "value": "554"
              },
              "PathCam2": {
                "value": "/video/0"
              },
              "connection_string": {
                "value": "DefaultEndpointsProtocol=https;AccountName=iaestoragedev;AccountKey=HACAChorM1ugThf0VsIEYnWFiYovsTQbnOxNUrClHJaa6+++h41nixS/8QYdqRKFH1A9YoONsEB3DGjy9/IiLw==;EndpointSuffix=core.windows.net"
              },
              "container": {
                "value": "image-frame"
              },
              "scheduleKey": {
                "value": "45x1b4g9SFXS3ZLEVpvq98i4uyVb8gbad0VbcAOavZYgE86JVRfLOQ4mm1H0jvz9AgHifIYNK9ekmmRsIXYRKg=="
              },
              "scheduleUrl": {
                "value": "https://iaepractiacosmosdb.documents.azure.com:443/"
              },
              "databaseIDCosmosDB": {
                "value": "framesAnalysis"
              },
              "containerIDCosmosDB": {
                "value": "schedulerDB"
              },
              "hostAddress": {
                "value": "owa.pragmaconsultores.net"
              },
              "hostPort": {
                "value": "2525"
              },
              "sender_email": {
                "value": "iae_ai_id_ar@practia.global"
              },
              "senderEmailPassword": {
                "value": "ID_510_mora"
              }
            },
            "settings": {
              "image": "${MODULES.FaceDetector}",
              "createOptions": {}
            }
          },
          "LogsModule": {
            "version": "3.1",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.LogsModule}",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"8080/tcp\":[{\"HostPort\":\"8080\"}]}}}"
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "FaceDetectorToIoTHub": "FROM /messages/modules/FaceDetector/outputs/* INTO $upstream",
          "LogsModuleToIoTHub": "FROM /messages/modules/LogsModule/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}